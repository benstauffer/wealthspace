// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}
datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
    relationMode = "prisma"
}

enum AccountType {
  CASH_ACCOUNT
  INVESTMENT
  PRIVATE_INVESTMENT
  REAL_ESTATE
  OTHER_ASSET
  PAYABLE
}

enum OwnerType {
    PERSONAL
    PARTNER
    OTHER
}

model Account {
  id          String          @id @default(cuid())
  owner       OwnerType
  type        AccountType
  name        String
  value       Float
}